#include<bits/stdc++.h>
using namespace std;
typedef long l;
typedef long long ll;
typedef unsigned long long ull;
typedef pair<int,int> pii;
typedef pair<long long,long long> pll;
 
ull n, a, b, x, y;
 
main(){
    cin>>a>>b>>x>>y;
 
    ull g= __gcd(x,y);
 
    x= x/g;
    y= y/g;
 
    cout<<min(a/x,b/y)<<endl;
 
}
